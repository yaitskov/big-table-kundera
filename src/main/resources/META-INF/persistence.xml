<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
        https://raw.github.com/impetus-opensource/Kundera/Kundera-2.0.4/kundera-core/src/test/resources/META-INF/persistence_2_0.xsd"
             version="2.0">
    <persistence-unit name="hbaseTest">
        <provider>com.impetus.kundera.KunderaPersistence</provider>
        <class>my.User</class>
        <properties>
            <property name="kundera.nodes" value="localhost"/>
            <property name="kundera.port" value="60000"/>
            <property name="kundera.keyspace" value="test2"/>
            <property name="kundera.dialect" value="hbase"/>
            <property name="kundera.client.lookup.class"
                      value="com.impetus.client.hbase.HBaseClientFactory"/>
            <property name="kundera.client.property" value="kunderaTest.xml"/>
        </properties>
    </persistence-unit>
</persistence>